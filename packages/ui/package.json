{
  "name": "@astral/ui",
  "browser": "./src/index.ts",
  "jest": {
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "prebuild": "npx rimraf ./lib && npm --prefix ../icons run svgr",
    "build": "commander build",
    "publish": "npm publish ./lib",
    "postbuild": "copyfiles -u 1 src/declaration/*.ts lib",
    "test": "react-scripts test --env=jsdom",
    "lint:types": "npm --prefix ../icons run svgr && tsc --noEmit",
    "lint": "eslint \"**/*.{ts,tsx}\" --fix --quiet",
    "lint:styles": "stylelint \"**/*.{ts,tsx}\" --fix"
  },
  "dependencies": {
    "@astral/icons": "^0.31.0",
    "@emotion/cache": "11.7.1",
    "@emotion/react": "11.9.0",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.8.1",
    "@mui/base": "^5.0.0-alpha.85",
    "@mui/lab": "^5.0.0-alpha.86",
    "@mui/material": "^5.8.4",
    "date-fns": "^2.28.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.0",
    "react-datepicker": "4.7.0",
    "react-imask": "^6.4.2",
    "react-toastify": "9.0.3",
    "react-use": "^17.4.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^12.12.38",
    "@types/react-datepicker": "4.4.0",
    "copyfiles": "^2.4.1"
  },
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./index.js"
    },
    "./server": {
      "import": "./esm/server/index.js",
      "require": "./server/index.js"
    }
  }
}