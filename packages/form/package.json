{
  "name": "@astral/form",
  "browser": "./src/index.ts",
  "main": "./src/index.ts",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "npx rimraf ./lib && npm --prefix ../icons run svgr",
    "postbuild": "copyfiles -u 1 declarations/*.ts lib",
    "build": "commander build",
    "publish": "npm publish ./lib",
    "lint:types": "npm --prefix ../icons run svgr && tsc --noEmit",
    "lint": "eslint --fix --quiet",
    "lint:all": "eslint \"**/*.{ts,tsx}\" --fix --quiet",
    "tests": "jest --passWithNoTests",
    "tests:ci": "npm --prefix ../icons run svgr && jest --ci --reporters=default --reporters=jest-junit"
  },
  "dependencies": {
    "react-hook-form": "^7.35.0",
    "@astral/ui": "*",
    "@astral/validations": "*"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@astral/tests": "*"
  }
}
