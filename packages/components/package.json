{
  "name": "@astral/components",
  "version": "2.0.0",
  "browser": "./src/index.ts",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./server": "./src/server/index.ts"
  },
  "typesVersions": {
    "*": {
      "server": [
        "src/server/index.ts"
      ],
      "declaration/emotion": [
        "src/declaration/emotion.d.ts"
      ],
      "declaration/mui-material": [
        "src/declaration/mui-material.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "node pack",
    "publish": "npm publish ./lib",
    "tests": "vitest run",
    "tests:ci": "vitest run --reporter=junit --outputFile.junit=junit.xml",
    "lint:types": "tsc --noEmit",
    "lint": "eslint --fix --quiet",
    "lint:all": "eslint \"**/*.{ts,tsx}\" --fix --quiet",
    "lint:styles": "stylelint --fix",
    "lint:styles:all": "stylelint \"**/*.{ts,tsx}\" --fix"
  },
  "dependencies": {
    "@astral/icons": "*",
    "@emotion/cache": "11.7.1",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@mui/base": "5.0.0-beta.0",
    "@mui/lab": "5.0.0-alpha.129",
    "@mui/material": "5.13.0",
    "@mui/system": "5.13.5",
    "dayjs": "^1.11.5",
    "lodash-es": "^4.17.21",
    "react-imask": "6.4.2",
    "react-toastify": "9.0.3",
    "react-use": "^17.4.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.5",
    "@astral/tests": "*"
  }
}
